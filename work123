exe '1resize ' . ((&lines * 36 + 55) / 111)
exe 'vert 1resize ' . ((&columns * 174 + 175) / 350)
exe '2resize ' . ((&lines * 36 + 55) / 111)
exe 'vert 2resize ' . ((&columns * 174 + 175) / 350)
exe '3resize ' . ((&lines * 35 + 55) / 111)
exe 'vert 3resize ' . ((&columns * 174 + 175) / 350)
