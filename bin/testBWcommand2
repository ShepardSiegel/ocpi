#!/bin/csh
# used for DMA throughput testing
echo > result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 16   -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 32   -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 64   -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 128  -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 256  -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 512  -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 1024 -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 2048 -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 4096 -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N0i 4 -I 8192 -i 1000 -z 0000:04:00.0 >>& result.log
sudo -E ./testRpl -r1im -r3om -N0o 4 -N1i 3 -N2o 3 -N0i 4 -I 8000 -i 1000 -z 0000:04:00.0 >>& result.log
grep Framesize result.log

