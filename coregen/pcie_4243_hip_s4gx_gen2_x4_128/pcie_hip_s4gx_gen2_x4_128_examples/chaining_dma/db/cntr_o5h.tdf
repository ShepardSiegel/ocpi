FUNCTION stratixiv_lcell_comb (cin, dataa, datab, datac, datad, datae, dataf, datag, sharein)
WITH ( DONT_TOUCH, EXTENDED_LUT, LUT_MASK, SHARED_ARITH)
RETURNS ( combout, cout, shareout, sumout);

--synthesis_resources = lut 2 reg 2 
OPTIONS ALTERA_INTERNAL_OPTION = "{-to counter_reg_bit1} POWER_UP_LEVEL=HIGH";

SUBDESIGN cntr_o5h
( 
	aset	:	input;
	clk_en	:	input;
	clock	:	input;
	cnt_en	:	input;
	cout	:	output;
) 
VARIABLE 
	counter_reg_bit0 : dffeas;
	counter_reg_bit1 : dffeas
		WITH (
			power_up = "high"
		);
	counter_comb_bita0 : stratixiv_lcell_comb
		WITH (
			EXTENDED_LUT = "off",
			LUT_MASK = "000000000000FF00",
			SHARED_ARITH = "off"
		);
	counter_comb_bita1 : stratixiv_lcell_comb
		WITH (
			EXTENDED_LUT = "off",
			LUT_MASK = "0000FF000000FF00",
			SHARED_ARITH = "off"
		);
	cout_actual	: WIRE;
	data[1..0]	: NODE;
	external_cin	: WIRE;
	lsb_cin	: WIRE;
	s_val[1..0]	: WIRE;
	sclr	: NODE;
	sload	: NODE;
	sset	: NODE;
	time_to_clear	: WIRE;
	updown_dir	: WIRE;
	updown_lsb	: WIRE;
	updown_other_bits	: WIRE;

BEGIN 
	counter_reg_bit[1..0].asdata = ((sset & s_val[]) # ((! sset) & data[]));
	counter_reg_bit[1..0].clk = clock;
	counter_reg_bit[0].clrn = (! aset);
	counter_reg_bit[1..0].d = ( counter_comb_bita[1..0].sumout);
	counter_reg_bit[1..0].ena = (clk_en & (((cnt_en # sclr) # sset) # sload));
	counter_reg_bit[1].prn = (! aset);
	counter_reg_bit[1..0].sclr = sclr;
	counter_reg_bit[1..0].sload = (sset # sload);
	counter_comb_bita[1..0].cin = ( counter_comb_bita[0].cout, lsb_cin);
	counter_comb_bita[1..0].datad = ( counter_reg_bit[1..0].q);
	counter_comb_bita[1..0].dataf = ( updown_other_bits, updown_lsb);
	cout = cout_actual;
	cout_actual = (((! counter_comb_bita[1].cout) $ updown_other_bits) # (time_to_clear & updown_dir));
	data[] = GND;
	external_cin = B"1";
	lsb_cin = B"0";
	s_val[] = B"11";
	sclr = GND;
	sload = GND;
	sset = GND;
	time_to_clear = B"0";
	updown_dir = B"0";
	updown_lsb = updown_dir;
	updown_other_bits = ((! external_cin) # updown_dir);
END;
