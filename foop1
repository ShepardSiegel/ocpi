exe '1resize ' . ((&lines * 12 + 42) / 84)
exe 'vert 1resize ' . ((&columns * 153 + 157) / 314)
exe '2resize ' . ((&lines * 7 + 42) / 84)
exe 'vert 2resize ' . ((&columns * 153 + 157) / 314)
exe '3resize ' . ((&lines * 61 + 42) / 84)
exe 'vert 3resize ' . ((&columns * 153 + 157) / 314)
